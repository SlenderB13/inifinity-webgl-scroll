extends ../base.pug

block variables
  - var template = 'home'

block body
  .home
    .home__content
      .home__content__title scroll to interact

      .home__gallery
        each image in images
          figure.home__gallery__media
            img.home__gallery__media__image(src=image)

      footer.home__footer
        p.home__footer__made-by made by Lucas Silva
        .home__footer__links
          a(href='https://www.instagram.com/reverendoslender/') instagram
          a(href='https://github.com/SlenderB13') github
